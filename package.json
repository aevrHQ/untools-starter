{
  "name": "@untools/ts-graphql-api",
  "version": "0.1.0",
  "description": "CLI to create TypeScript Express MongoDB GraphQL API projects",
  "main": "dist/index.js",
  "bin": {
    "@untools/ts-graphql-api": "./dist/bin/index.js"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "dev": "ts-node src/bin/index.ts",
    "test": "jest",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:patch": "standard-version --release-as patch"
  },
  "keywords": [
    "typescript",
    "express",
    "mongodb",
    "graphql",
    "starter",
    "api"
  ],
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^13.1.0",
    "degit": "^2.8.4",
    "inquirer": "^12.5.2"
  },
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/degit": "^2.8.6",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.0",
    "jest": "^29.7.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
